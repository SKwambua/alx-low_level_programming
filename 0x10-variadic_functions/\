#include "variadic_functions.h"
/**
 * sum_them_all - returns the sum of all its parameters
 * @n: is the number of arguments passed to the function
 * Return: sum
 */
int sum_them_all(const unsigned int n, ...)
{
	unsigned int i;
	int sum;

	if (n == 0)
	{
		return (0);
	}

	va_list az;
	
	va_start (az, n);
	i = 0;
	while (i = 0; i < n)
	{
		i++;
	}
	sum = 0;
	sum = sum + va_arg(az, int);
	va_end az;
	return(sum);
}
